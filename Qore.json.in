{
    \"Name\" : \"Qore\",
    \"Version\" : \"0.0.1\",
    \"CompatVersion\" : \"0.0.1\",
    \"Vendor\" : \"Petr Vanek\",
    \"Copyright\" : \"Petr Vanek\",
    \"License\" : \"MIT\",
    \"Description\" : \"Qore language plugin\",
    \"Category\" : \"Other Languages\",
    \"Url\" : \"http://qore.org\",
    $$dependencyList,

    \"Mimetypes\" : [
        \"<?xml version=\'1.0\'?>\",
        \"<mime-info xmlns=\'http://www.freedesktop.org/standards/shared-mime-info\'>\",

        \"    <mime-type type=\'text/x-qore-project\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>Qore project file</comment>\",
        \"        <glob pattern=\'*.qoreproject\'/>\",
        \"    </mime-type>\",

        \"    <mime-type type=\'text/x-qore\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>Qore source file</comment>\",
        \"        <magic priority=\'50\'>\",
        \"            <match value=\'#!/usr/bin/env qore\' type=\'string\' offset=\'0:25\'/>\",
        \"            <match value=\'qore\' type=\'string\' offset=\'0:25\'/>\",
        \"            <match value=\'%requires \' type=\'string\' offset=\'0:500\'/>\",
        \"            <match value=\'%include \' type=\'string\' offset=\'0:500\'/>\",
        \"            <match value=\'%new-style\' type=\'string\' offset=\'0:500\'/>\",
        \"            <match value=\'%exec-class\' type=\'string\' offset=\'0:500\'/>\",
        \"            <match value=\'mode: qore\' type=\'string\' offset=\'0:50\'/>\",
        \"        </magic>\",
        \"        <glob pattern=\'*.q*\'/>\",
        \"    </mime-type>\",

        \"</mime-info>\"
    ]
}
